{
  "name": "frontend",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "unit-tests-once": "ng test --watch=false --browsers=ChromeHeadless --code-coverage",
    "e2e": "ng e2e",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run --spec 'cypress/e2e/mocked.cy.ts' --headless --browser chrome",
    "cypress:run-unmocked": "cypress run --spec 'cypress/e2e/unmocked.cy.ts' --headless --browser chrome",
    "e2e-mocked": "start-server-and-test start http://localhost:4200 cypress:run",
    "e2e-unmocked": "start-server-and-test start http://localhost:4200 cypress:run-unmocked"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^14.1.0",
    "@angular/common": "^14.1.0",
    "@angular/compiler": "^14.1.0",
    "@angular/core": "^14.1.0",
    "@angular/forms": "^14.1.0",
    "@angular/platform-browser": "^14.1.0",
    "@angular/platform-browser-dynamic": "^14.1.0",
    "@angular/router": "^14.1.0",
    "canvas-confetti": "^1.6.0",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.1.1",
    "@angular/cli": "~14.1.1",
    "@angular/compiler-cli": "^14.1.0",
    "@cypress/schematic": "^2.4.0",
    "@types/canvas-confetti": "^1.6.0",
    "@types/jasmine": "~4.0.0",
    "cypress": "latest",
    "jasmine-core": "~4.2.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "start-server-and-test": "^1.15.2",
    "typescript": "~4.7.2"
  }
}
